{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "revadfproj"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase'"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub1'"
		},
		"Httptogithub_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Httptogithub'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://revadfprojdatalake.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://revprojkeyvlt.vault.azure.net/"
		},
		"AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "revaserver.database.windows.net"
		},
		"AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "revadbazure"
		},
		"AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "revadmin"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Reva011190"
		},
		"Httptogithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"Httptogithub_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Reva011190"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "revadfproj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabase_password": {
						"value": "[parameters('AzureSqlDatabase_password')]"
					},
					"GitHub1_credential": {
						"value": "[parameters('GitHub1_credential')]"
					},
					"Httptogithub_password": {
						"value": "[parameters('Httptogithub_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"GitHub1_properties_typeProperties_username": {
						"value": "[parameters('GitHub1_properties_typeProperties_username')]"
					},
					"Httptogithub_properties_typeProperties_url": {
						"value": "[parameters('Httptogithub_properties_typeProperties_url')]"
					},
					"Httptogithub_properties_typeProperties_userName": {
						"value": "[parameters('Httptogithub_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "revadfproj_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'revadfproj_ArmTemplate_0')]"
			]
		}
	]
}